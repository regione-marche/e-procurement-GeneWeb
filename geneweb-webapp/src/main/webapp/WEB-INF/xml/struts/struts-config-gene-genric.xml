		<!--   Dettaglio Ricerca   -->
		<action path="/DettaglioRicerca"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false" parameter="metodo">
			<forward name="successVisualizza"	path="/CambiaTabRicerca.do" />
			<forward name="successCopia" path="/TrovaRicerche.do?metodo=trovaRicerche" />
			<forward name="successSalva" path="/CambiaTabRicerca.do" />
			<forward name="successSalvaETrova"  path="/InitTrovaRicerche.do" />
			<forward name="successSalvaELista"  path="/TrovaRicerche.do?metodo=trovaRicerche" />
			<forward name="successSalvaECrea"	  path="/DettaglioRicerca.do?metodo=crea" />
			<forward name="successSalvaEEsegui"	path="/ControllaDatiRicerca.do" />
			<forward name="tornaATrovaRicerche" path="/InitTrovaRicerche.do" />
			<forward name="tornaAListaRicerche" path="/TrovaRicerche.do?metodo=trovaRicerche" />
			<forward name="tornaDettaglioRicercaPrecedente" path="/DettaglioRicerca.do?metodo=visualizza" />
			<forward name="tornaAListaRicerchePredefinite" path="/ListaRicerchePredefinite.do" /> 
			<forward name="tornaAHomePage" path="/Home.do" module="/" />
			<forward name="errorSalva" path="/CambiaTabRicerca.do" />
			<forward name="successCaricaPerEstrazione" path="/ControllaDatiRicercaPredefinita.do" />
			<forward name="successCaricaEEsegui" path="/ControllaDatiRicerca.do" />
			<forward name="forwardDatiGenProspetto" path="/DettaglioProspetto.do?metodo=visualizza" />
			<forward name="forwardEseguiProspettoPredefinito" path="/CheckParametriProspettoPredefinito.do" />
			<forward name="errorSalvaReportBase" path="/CambiaTabRicercaBase.do" />
			<forward name="successSalvaReportBase" path="/CambiaTabRicercaBase.do" />
			<forward name="successSalvaReportSql" path="/CambiaTabRicercaSql.do" />
			<forward name="errorSalvaReportSql" path="/CambiaTabRicercaSql.do" />
		</action>
		
		<action path="/CreaEliminaRicerca"
			type="org.springframework.web.struts.DelegatingActionProxy"
