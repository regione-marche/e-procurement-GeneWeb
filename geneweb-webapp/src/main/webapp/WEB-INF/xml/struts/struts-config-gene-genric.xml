			<forward name="success" path="/EseguiImport.do"/>
		</action>

		<action path="/EseguiImport"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false">
			<forward name="success" path="successImportRicercaDef" />
