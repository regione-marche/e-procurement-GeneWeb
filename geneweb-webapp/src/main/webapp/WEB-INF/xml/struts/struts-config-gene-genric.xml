	<action-mappings>
		<action path="/InitTrovaRicerche" name="trovaRicercheForm"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false">
			<forward name="success" path="trovaRicercheDef" />
		</action>

		<action path="/TrovaRicerche" name="trovaRicercheForm"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false" parameter="metodo">
			<forward name="success" path="trovaRicercheDef" />
			<forward name="successListaRicerche"
				path="listaRicercheDef" />
			<forward name="successTrovaRicerche"
				path="/InitTrovaRicerche.do" />
			<forward name="noRecordFound"
				path="/InitTrovaRicerche.do" />
		</action>

		<action path="/ListaRicerche" name="listaForm"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false" parameter="metodo">
			<forward name="successElimina"
				path="/TrovaRicerche.do?metodo=trovaRicerche"/>
		</action>

		<action path="/CopiaProspetto"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false">
			<forward name="success" path="/TrovaRicerche.do?metodo=trovaRicerche"/>
		</action>

		<action path="/CreaNuovaRicerca"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false">
			<forward name="success" path="creaNuovaRicercaDef" />
			<forward name="successBase"	path="/CreaEliminaRicercaBase.do?metodo=crea&amp;famiglia=0" />
			<forward name="successAvanzato"	path="/CreaEliminaRicerca.do?metodo=crea&amp;famiglia=1" />
			<forward name="successProspetto"	path="/CreaEliminaRicercaProspetto.do?metodo=crea&amp;famiglia=2" />
			<forward name="successReportSql"	path="/CreaEliminaRicercaSql.do?metodo=crea&amp;famiglia=4" />
		</action>
		
		<action path="/CaricaRicerca"
		  type="org.springframework.web.struts.DelegatingActionProxy"
		  scope="request" validate="false">
		  <forward name="successBase" path="/WizardBase.do" />
		  <forward name="successAvanzata" path="/EditDatiGen.do" />
		</action>
		
