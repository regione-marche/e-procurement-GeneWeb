			<forward name="lista" 
					path="/TrovaRicerche.do?metodo=trovaRicerche"/>
			<forward name="successCreaReportBase" path="/EditDatiGen.do"/>
			<forward name="successCrea" path="/EditDatiGen.do"/>
			<forward name="successCreaProspetto" path="/EditDatiGenProspetto.do" />
			<forward name="successCreaReportSql" path="/EditDatiGen.do"/>
		</action>
		
		<action path="/CambiaTabRicerca"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false">
			<forward name="datiGenerali" path="dettaglioDatiGenRicercaDef" />
			<forward name="gruppi" path="listaGruppiRicercaDef" />
			<forward name="argomenti" path="listaArgomentiRicercaDef" />
			<forward name="campi" path="listaCampiRicercaDef" />
			<forward name="join" path="listaGiunzioniRicercaDef" />
			<forward name="ordinamenti" path="listaOrdinamentiRicercaDef" />
			<forward name="filtri" path="listaFiltriRicercaDef" />
			<forward name="parametri" path="listaParametriRicercaDef" />
			<forward name="layout" path="listaTitoliCampiRicercaDef" />
			<forward name="sql" path="dettaglioSqlRicercaSqlDef" />
		</action>

