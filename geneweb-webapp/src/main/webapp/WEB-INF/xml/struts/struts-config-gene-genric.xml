			<forward name="errorImport" path="/WizardImportRicerca.do?pageTo=UPL" />
		</action>

		<action path="/SetOpzioniImport" name="opzioniImportForm"
			type="org.springframework.web.struts.DelegatingActionProxy"
			scope="request" validate="false">
			<forward name="successDomandaPub" path="/WizardImportRicerca.do?pageTo=PUB"/>
			<forward name="successEditGrp" path="/WizardImportRicerca.do?pageTo=GRP"/>
			<forward name="successFine" path="/EseguiImport.do"/>
			<forward name="errorImport" path="/WizardImportRicerca.do?pageTo=DG"/>
