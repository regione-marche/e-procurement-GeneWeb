<?xml version="1.0" encoding="UTF-8" ?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/
   web-jsptaglibrary_2_0.xsd" version="2.0">

	<tlib-version>1.0.0</tlib-version>

	<jsp-version>1.2</jsp-version>

	<short-name>genetags</short-name>

	<uri>http://www.eldasoft.it/genetags</uri>

	<description>Tag Library per le funzionalita' generali</description>
	

	<tag>
		<name>voceMenu</name>
		<tag-class>it.eldasoft.gene.tags.decorators.VoceMenuTag</tag-class>
		<body-content>JSP</body-content>
		<description>
		  Creazione di una voce in alto sul menu a tendina
			Parametri di input:
			id			Eventuale identificativo
			value		Valore della voce di menu
			title		Titolo
			index		indice
			abilitato flag se alilitare o meno il menu
		</description>

		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>etichetta</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>index</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>abilitato</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

	</tag>
	
	<tag>
		<name>formSelect</name>
		<tag-class>it.eldasoft.gene.tags.decorators.lista.FormSelectTag</tag-class>
		<body-content>JSP</body-content>
		<description>
		  Select diretta sul database. (utilizzata nella lista).
		</description>
	</tag>
	
	<tag>
		<name>voceSubMenu</name>
		<tag-class>it.eldasoft.gene.tags.js.VoceSubMenuTag</tag-class>
		<body-content>empty</body-content>
		<description>
		  Creazione della voce nel menu a tendina per il richiamo di una pagina partendo dal nome di un archivio
			tutti i dati vengono estratti dai metadati
			Parametri:
				etichetta
					Obblig. Etichetta del menu
				index
					tabindex
				title
					Titolo
				href
					Pagina da chiamare. Al posto dell'espressione $CONTEXT$ mette il page Context
				abilitato
					Abilitazione del menu
		</description>

		<attribute>
			<name>etichetta</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>index</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>href</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>abilitato</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
      <name>gestisciProtezioni</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.lang.Boolean</type>
      <description>
				Flag che dice se gestire o meno le protezioni sul campo. (true di default)
      </description>
    </attribute>
		
		<attribute>
			<name>idProtezioni</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>
        Identificativo delle protezioni
      </description>
		</attribute>
		
	</tag>
	
	<tag>
		<name>voceSubMenuArchivio</name>
		<tag-class>it.eldasoft.gene.tags.js.VoceSubMenuArchivioTag</tag-class>
		<body-content>empty</body-content>
		<description>
			Creazione di una voce di sottomenu partendo da un archivio nello schema concettuale
		  Parametri:
				tabella
					Obblig. Nome fisico della tabella

		</description>

		<attribute>
			<name>tabella</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<name>abilitato</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
      <name>gestisciProtezioni</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.lang.Boolean</type>
      <description>
				Flag che dice se gestire o meno le protezioni sul campo. (true di default)
      </description>
    </attribute>
		
		<attribute>
			<name>idProtezioni</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>
        Identificativo delle protezioni
      </description>
		</attribute>
		
	</tag>
	
	<tag>
		<name>template</name>
		<tag-class>it.eldasoft.gene.tags.templates.JspTemplateTag</tag-class>
		<body-content>JSP</body-content>
		<description>
			Gestore dei template da utilizzare
			Parametri:
				file
