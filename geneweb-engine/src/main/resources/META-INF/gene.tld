	<!-- Lista di tabella -->
	<tag>
    <name>formLista</name>
    <tag-class>it.eldasoft.gene.tags.decorators.lista.FormListaTag</tag-class>
    <body-content>JSP</body-content>
    <display-name>formLista</display-name>
    <description>
      Lista di un'entita'. Durante le interazioni vengono settati i seguenti campi nel response
			chiaveRiga 
				Stringa con definita la chiave della riga corrente
			currentRow
				Numero delle riga corrente
    </description>
		<attribute>
      <name>entita</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Entita principale della lista. Es. PERI
      </description>
    </attribute>
		<attribute>
      <name>varName</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Nome della variabile che contiene i dati della lista
      </description>
    </attribute>
		<attribute>
			<name>sql</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>
				Eventuale Sql da eseguire per l'estrazione dei dati. (preferibilmente utilizzare formSelect all'interno della lista)
      </description>
		</attribute>
		
		<attribute>
      <name>pagesize</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Dimensione delle pagina a righe.
      </description>
    </attribute>
		<attribute>
      <name>tableclass</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Classe da utilizzare nella tabella
      </description>
    </attribute>
		<attribute>
      <name>where</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
        Where da aggiungere alla lista per eventuali filtri aggiuntivi sulla lista
      </description>
    </attribute>
		<attribute>
			<name>gestore</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Gestore utilizzato per personalizzare le procedura di insert ed update e delete.
				Questo gestore deve estendere la classe: it.eldasoft.gene.web.struts.tags.gestori.AbstractGestoreEntita
      </description>
		</attribute>
		<attribute>
			<name>pathScheda</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>
        Path della scheda di default di apertura.
      </description>
		</attribute>
		<attribute>
			<name>pathSchedaPopUp</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>
        Path della scheda di default in apertura durante un popUp.
      </description>
		</attribute>
