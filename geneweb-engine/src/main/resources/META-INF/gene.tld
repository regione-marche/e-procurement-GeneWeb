	<!-- gestione dei wizard -->
	<tag>
    <name>wizard</name>
    <tag-class>it.eldasoft.gene.tags.decorators.wizard.WizardTag</tag-class>
    <body-content>JSP</body-content>
    <display-name>wizard</display-name>
    <description>
      Tag che gestisce una maschera di una creazione guidata costituita da piu' passi
    </description>
		<attribute>
      <name>gestisciProtezioni</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.lang.Boolean</type>
      <description>
				Flag che dice se gestire o meno le protezioni sul campo. (true di default)
      </description>
    </attribute>
		<attribute>
      <name>entitaPrincipale</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
				Entita' principale associata al wizard
      </description>
    </attribute>
		<attribute>
      <name>gestoreSalvataggio</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
				Nome della classe, comprensiva del package, del gestore da richiamare al
   				salvataggio dei dati nel wizard
   				Questo gestore deve estendere la classe: it.eldasoft.gene.web.struts.tags.gestori.AbstractGestoreEntita
      </description>
    </attribute>
		<attribute>
      <name>hrefFineWizard</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <description>
				Pagina da richiamare al termine del wizard. Di default, se non specificato, si carica la scheda in visualizzazione  
      </description>
    </attribute>
  </tag>
	
	<tag>
    <name>paginaWizard</name>
    <tag-class>it.eldasoft.gene.tags.decorators.wizard.PaginaWizardTag</tag-class>
    <body-content>JSP</body-content>
    <display-name>paginaWizard</display-name>
    <description>
      Pagina di una creazione guidata
    </description>
		<attribute>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Titolo delle pagina
      </description>
		</attribute>
		<attribute>
			<name>visibile</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
			<description>
        Flag che dice che la pagina e' visibile (default true)
      </description>
		</attribute>
		<attribute>
			<name>idProtezioni</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
			<description>
        Identificativo delle protezioni
      </description>
		</attribute>		
  </tag>	

	<tag>
    <name>formSchedaWizard</name>
    <tag-class>it.eldasoft.gene.tags.decorators.wizard.FormSchedaWizardTag</tag-class>
    <body-content>JSP</body-content>
    <display-name>formSchedaWizard</display-name>
    <description>
      Tag che gestisce una scheda di dati in un wizard
    </description>
		<attribute>
			<name>entita</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Entita principale della scheda.
      </description>
		</attribute>
		<attribute>
			<name>plugin</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Gestore utilizzato per personalizzare il caricamento della pagina introducendo attributi negli oggetti impliciti ed eseguendo query
				Questo gestore deve estendere la classe: it.eldasoft.gene.web.struts.tags.gestori.AbstractGestorePreload
      </description>
		</attribute>
		<attribute>
			<name>gestoreNavigazione</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Gestore utilizzato per personalizzare i comandi nella pagina (Avanti, Indietro, Fine, Salva)
				Questo gestore deve estendere la classe: it.eldasoft.gene.web.struts.tags.AbstractCustomWizardAction
      </description>
		</attribute>
		<attribute>
      <name>gestisciProtezioni</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.lang.Boolean</type>
      <description>
				Flag che dice se gestire o meno le protezioni sul campo. (true di default)
      </description>
    </attribute>
		<attribute>
      <name>tableClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Eventuale classe di visualizzazione del tab. (default e' dettaglio-tab)
      </description>
		</attribute>
		<attribute>
      <name>tipoPagina</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Codice che identifica la tipologia della pagina. I valori standard sono
        - DETTAGLIO
        - LISTA_DOMANDA
        - LISTA_NUOVO
      </description>
		</attribute>
  </tag>
  
	<tag>
    <name>getDatoWizard</name>
    <tag-class>it.eldasoft.gene.tags.decorators.wizard.GetDatoWizardTag</tag-class>
    <body-content>JSP</body-content>
    <display-name>getDatoWizard</display-name>
    <description>
      Estrae, dall'oggetto presente in sessione e rappresentante la collezione di dati 
      utilizzati ed impostati dal wizard, un'informazione.
    </description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Identificativo della variabile di scripting da definire
      </description>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Nome dell'attributo da leggere in sessione
      </description>
		</attribute>
		<attribute>
			<name>scope</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
        Visibilita' della variabile creata. I valori possibili sono:
        - page (DEFAULT)
        - request
        - session
        - application
      </description>
		</attribute>		
  </tag>	
  
  	
</taglib>
